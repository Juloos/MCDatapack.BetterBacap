$data modify storage bac_display:data min_char_len.$(team).blank set value '$(blank)'
$data modify storage bac_display:data min_char_len.$(team).dash set value '$(dash)'
$data modify storage bac_display:data min_char_len.$(team).double_dash set value '$(double_dash)'
$data modify storage bac_display:data min_char_len.$(team).tilde set value '$(tilde)'
$data modify storage bac_display:data min_char_len.$(team).underscore set value '$(underscore)'
